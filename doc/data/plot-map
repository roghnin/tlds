set format y "%.0s%c"
set ylabel "Throughput (OP/s)" offset (0,0)


plot "walltime_filled_TRANSMAP_key_1000000_iter_1000000_ins_15_del_5_upd_5" u ($0+1):($3/$2)   w linespoints ls 1 pt 1 title "LFT-1",\
     "walltime_filled_TRANSMAP_key_1000000_iter_1000000_ins_15_del_5_upd_5" u ($0+1):(2*$6/$5)   w linespoints ls 2 pt 2 title "LFT-2",\
     "walltime_filled_TRANSMAP_key_1000000_iter_1000000_ins_15_del_5_upd_5" u ($0+1):(4*$9/$8)   w linespoints ls 3 pt 3 title "LFT-4",\
     "walltime_filled_TRANSMAP_key_1000000_iter_1000000_ins_15_del_5_upd_5" u ($0+1):(8*$12/$11) w linespoints ls 4 pt 4 title "LFT-8",\
     "walltime_filled_TRANSMAP_key_1000000_iter_1000000_ins_15_del_5_upd_5" u ($0+1):(16*$15/$14) w linespoints ls 5 pt 6 title "LFT-16"


set key inside top vertical left Left reverse samplen 2 height 0.5 spacing 1

set lmargin 0
set rmargin 0
set tmargin 0
set bmargin 0

if(0) {
