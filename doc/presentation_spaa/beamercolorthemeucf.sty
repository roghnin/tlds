\mode<presentation>


\definecolor{ucfgold}{RGB}{188,154,0}
\definecolor{ucfblack}{RGB}{48,48,48}

\setbeamercolor{item}{fg=ucfgold}

\setbeamercolor{structure}{fg=ucfblack}

\setbeamercolor{palette primary}{fg=ucfblack,bg=ucfgold!40}
\setbeamercolor{palette secondary}{fg=ucfblack,bg=ucfgold!60}
\setbeamercolor{palette tertiary}{fg=ucfblack,bg=ucfgold!80}
\setbeamercolor{palette quaternary}{fg=ucfblack,bg=ucfgold}

\setbeamercolor{titlelike}{parent=palette quaternary}

\setbeamercolor{block title}{fg=ucfblack,bg=ucfgold}
\setbeamercolor{block title alerted}{use=alerted text,fg=ucfblack,bg=alerted text.fg!75!bg}
\setbeamercolor{block title example}{use=example text,fg=ucfblack,bg=example text.fg!75!bg}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!25!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!25!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!25!bg}

\setbeamercolor{sidebar}{bg=ucfgold!70}
  
\setbeamercolor{palette sidebar primary}{fg=ucfblack}
\setbeamercolor{palette sidebar secondary}{fg=ucfblack!75}
\setbeamercolor{palette sidebar tertiary}{fg=ucfblack!75}
\setbeamercolor{palette sidebar quaternary}{fg=ucfblack}

%\setbeamercolor*{separation line}{}
%\setbeamercolor*{fine separation line}{}

\mode
<all>
